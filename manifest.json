{
    "manifest_version": 2,
    "name": "Lola",
    "version": "0.2.5",
    "description": "Lola vous permet de changer le style de la page \"Nouvel Onglet\"",
    "author": "Nicolas Guilloux",
    "developer": {
        "name": "Nicolas Guilloux",
        "url": "https://nicolasguilloux.eu/"
    },
    "icons": {
        "48": "img/icon-48.png",
        "96": "img/icon-96.png"
    },
    "applications": {
        "gecko": {
            "id": "lola@nicolasguilloux.eu",
            "strict_min_version": "57.0a1"
        }
    },
    "permissions": [ "storage" ],
    "options_ui": {
        "page": "options.html",
        "browser_style": true
    },
    "chrome_url_overrides" : {
        "newtab": "app.html"
    },
    "chrome_settings_overrides" : {
        "homepage": "app.html"
    }
}
